{
  "name": "vehkavaraus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "devdb": "docker-compose -f compose.dev.yml up",
    "testdb": "docker-compose -f compose.test.yml up",
    "resetdb": "rm -rf ./database/ && docker-compose -f compose.dev.yml down && docker volume rm vehkavaraus_db_data",
    "enterdb": "docker exec -it vehkavaraus_db psql -U postgres",
    "entertestdb": "docker exec -it vehkavaraus_test psql -U postgres",
    "test": " vitest",
    "coverage": "vitest run --coverage",
    "migrate": "tsx ./src/database/migrations/utils/migrate.ts",
    "migrate:down": "tsx ./src/database/migrations/utils/migrateDown.ts",
    "migrate:test": "cross-env NODE_ENV=test tsx ./src/database/migrations/utils/migrate.ts",
    "checkTypes": "tsc --noEmit"
  },
  "dependencies": {
    "@react-email/components": "^0.0.28",
    "@vercel/blob": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "next": "15.0.4",
    "next-auth": "^4.24.10",
    "pg": "^8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "resend": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/env": "15.0.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.6",
    "@types/pg": "^8.11.10",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@vitest/coverage-v8": "2.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.4",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.5"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.1"
    }
  }
}
