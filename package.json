{
  "name": "vehkavaraus",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "devdb": "docker-compose -f compose.dev.yml up",
    "testdb": "docker-compose -f compose.test.yml up",
    "resetdb": "rm -rf ./database/ && docker-compose -f compose.dev.yml down && docker volume rm vehkavaraus_db_data",
    "enterdb": "docker exec -it vehkavaraus_db psql -U postgres",
    "entertestdb": "docker exec -it vehkavaraus_test psql -U postgres",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "next": "14.2.13",
    "next-auth": "^4.24.8",
    "pg": "^8.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.47",
